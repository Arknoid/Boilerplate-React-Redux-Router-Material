{
    "extends": [
        "prettier",
        "prettier/react",
        "prettier/standard",
        "plugin:react/recommended",
        "plugin:import",
        "plugin:jsx-a11y"
    ],
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ],
    "parser": "babel-eslint",
    "rules": {
        "react-hooks/rules-of-hooks": "error", 
        "react-hooks/exhaustive-deps": "warn"
    }
}